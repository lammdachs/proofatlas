{
  "version": "3.9",
  "source": "http://www.spass-prover.org/download/sources/spass39.tgz",

  "paths": {
    "binary": ".spass/SPASS"
  },

  "notes": {
    "selection_mapping": "SPASS Select=0 ~ Vampire sel20, Select=1 ~ sel21",
    "no_sel0": "SPASS does not support selecting all literals (Vampire sel0)"
  },

  "default_preset": "spass_time",

  "presets": {
    "spass_time": {
      "description": "10s",
      "TimeLimit": 10,
      "Select": 1
    },
    "spass_age_weight": {
      "description": "256 loops",
      "TimeLimit": 600,
      "Loops": 256,
      "Select": 1
    }
  }
}
