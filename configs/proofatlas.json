{
  "defaults": {
    "preset": "time"
  },

  "presets": {
    "time": {
      "description": "10s timeout",
      "timeout": 10,
      "literal_selection": 21,
      "age_weight_ratio": 0.5
    },

    "age_weight": {
      "description": "Age-weight baseline (512 iterations)",
      "timeout": 600,
      "max_iterations": 512,
      "literal_selection": 21,
      "age_weight_ratio": 0.5,
      "memory_limit": 512
    },

    "gcn_mlp": {
      "description": "GCN + MLP",
      "timeout": 600,
      "max_iterations": 512,
      "literal_selection": 21,
      "encoder": "gcn",
      "scorer": "mlp",
      "traces": "age_weight"
    },

    "gcn_attention": {
      "description": "GCN + attention",
      "timeout": 600,
      "max_iterations": 512,
      "literal_selection": 21,
      "encoder": "gcn",
      "scorer": "attention",
      "traces": "age_weight"
    },

    "gcn_transformer": {
      "description": "GCN + transformer",
      "timeout": 600,
      "max_iterations": 512,
      "literal_selection": 21,
      "encoder": "gcn",
      "scorer": "transformer",
      "traces": "age_weight"
    },

    "sentence_mlp": {
      "description": "Sentence + MLP",
      "timeout": 600,
      "max_iterations": 512,
      "literal_selection": 21,
      "encoder": "sentence",
      "scorer": "mlp",
      "traces": "age_weight"
    }
  }
}
