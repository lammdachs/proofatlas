{
  "paths": {
    "binary": "rust/target/release/prove"
  },

  "defaults": {
    "preset": "time_sel21"
  },

  "presets": {
    "time_sel0": {
      "description": "10s with sel0 (all literals)",
      "timeout": 10,
      "literal_selection": 0,
      "age_weight_ratio": 0.167
    },

    "time_sel20": {
      "description": "10s with sel20 (all maximal literals)",
      "timeout": 10,
      "literal_selection": 20,
      "age_weight_ratio": 0.167
    },

    "time_sel21": {
      "description": "10s with sel21 (unique maximal, else neg max-weight, else all maximal)",
      "timeout": 10,
      "literal_selection": 21,
      "age_weight_ratio": 0.167
    },

    "steps_sel0": {
      "description": "128 iterations with sel0",
      "timeout": 600,
      "max_iterations": 128,
      "literal_selection": 0,
      "age_weight_ratio": 0.167
    },

    "steps_sel20": {
      "description": "128 iterations with sel20",
      "timeout": 600,
      "max_iterations": 128,
      "literal_selection": 20,
      "age_weight_ratio": 0.167
