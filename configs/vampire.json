  "paths": {
    "binary": ".vampire/vampire"
  },

  "default_preset": "time_sel21",

  "presets": {
    "time_sel0": {
      "description": "10s with selection 0 (all literals)",
      "time_limit": 10,
      "selection": 0,
      "avatar": "off"
    },

    "time_sel20": {
      "description": "10s with selection 20 (all maximal literals)",
      "time_limit": 10,
      "selection": 20,
      "avatar": "off"
    },

    "time_sel21": {
      "description": "10s with selection 21 (unique maximal, else neg max-weight, else all maximal)",
      "time_limit": 10,
      "selection": 21,
      "avatar": "off"
    },

    "time_sel22": {
      "description": "10s with selection 22 (neg max-weight, else all maximal)",
      "time_limit": 10,
      "selection": 22,
      "avatar": "off"
    }
  }
