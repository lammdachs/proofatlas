{
  "version": "5.0.0",
  "sources": {
    "linux-x86_64": "https://github.com/vprover/vampire/releases/download/v5.0.0/vampire-Linux-X64.zip",
    "linux-aarch64": "https://github.com/vprover/vampire/releases/download/v5.0.0/vampire-Linux-ARM64.zip"
  },

  "paths": {
    "binary": ".vampire/vampire"
  },

  "default_preset": "time",

  "presets": {
    "time": {
      "description": "10s",
      "time_limit": 10,
      "selection": 21,
      "avatar": "off",
      "saturation_algorithm": "otter"
    },
    "age_weight": {
      "description": "256 activations",
      "time_limit": 600,
      "activation_limit": 256,
      "selection": 21,
      "avatar": "off",
      "saturation_algorithm": "otter"
    }
  }
}
